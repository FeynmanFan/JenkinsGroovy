node {
    stage('SCM') {
	echo 'Gathering code from version control'
	git branch: '${Branch}', url: 'https://github.com/rlp2731/JenkinsGroovy.git'
    }
    stage('Build') {
        echo 'Building....'
        echo 'Building new feature...'
        sh 'dotnet --version'
        sh dotnet build 'ConsoleApp1'
    }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}